    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.3.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "react",
    "search",
    "modal",
    "component"
